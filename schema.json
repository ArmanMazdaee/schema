        "additionalProperties": true,
        "properties": {
          "company": {
            "description": "e.g. Xyz",
            "type": "string"
          },
          "name": {
            "description": "e.g. Stephan Mark",
            "type": "string"
          },
          "position": {
            "description": "e.g. Senior Software Engineer",
            "type": "string"
          },
          "reference": {
