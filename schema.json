          "endDate": {
            "description": "e.g. 2017-06-29 - [resume.json uses the ISO 8601 date standard] ",
            "format": "date",
            "type": "string"
          },
          "entity": {
            "description": "e.g. 'greenpeace', 'corporationXYZ' - [Relevant company/entity affiliations]",
